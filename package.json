{
  "name": "chatts",
  "version": "1.0.0",
  "description": "Simple chat using Express, Socket.io, and TypeScript (MVC)",
  "main": "dist/server.ts",
  "scripts": {
    "dev": "nodemon --watch src --ext ts --exec ts-node src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "tunnel": "npx ngrok@latest http 3000"
  },
  "keywords": [
    "chat",
    "express",
    "socket.io",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "ngrok": "^5.0.0-beta.2",
    "socket.io": "^4.7.5"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.17",
    "nodemon": "^3.0.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
